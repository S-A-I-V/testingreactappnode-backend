{
    "version": 2,
    "builds": [
      {
        "src": "testingreactappnode/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "testingreactappnode/server.js"
      }
    ]
  }
  